# Greatest Common Divisor using Euclidean algorithm

fn gcd(a, b) {
  while b != 0 {
    let temp = b
    b = a % b
    a = temp
  }
  return a
}

print gcd(48, 18)
print gcd(100, 35)
print gcd(1071, 462)

# LCM using GCD
fn lcm(a, b) {
  return a * b / gcd(a, b)
}

print lcm(12, 18)
print lcm(21, 6)

